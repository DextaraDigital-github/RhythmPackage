<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_Duplicate_Template</fullName>
    <active>true</active>
    <errorConditionFormula>VLOOKUP($ObjectType.Assessment_Template__c.Fields.Name,$ObjectType.Assessment_Template__c.Fields.Name, Name ) == Name</errorConditionFormula>
    <errorMessage>Template exists with the same name already, try using a different name.</errorMessage>
</ValidationRule>
