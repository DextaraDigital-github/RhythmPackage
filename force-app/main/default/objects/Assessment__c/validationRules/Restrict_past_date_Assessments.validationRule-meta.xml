<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_past_date_Assessments</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Number_of_invited_Suppliers__c ==0,ISCHANGED(Start_Date__c),Start_Date__c &lt; TODAY())</errorConditionFormula>
    <errorDisplayField>Start_Date__c</errorDisplayField>
    <errorMessage>Start Date of an Assessment Program cannot be a Past Date</errorMessage>
</ValidationRule>
