<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_past_end_date_Assessments</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISNEW()),ISCHANGED(End_Date__c),End_Date__c &lt;  TODAY())</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>End Date cannot be a past date</errorMessage>
</ValidationRule>
